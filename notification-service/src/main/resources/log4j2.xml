<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
  <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <!--      <PatternLayout-->
      <!--        pattern="%d{yyyy/MM/dd HH:mm:ss},%X{uri},%X{mobileTraceId},%X{beTraceId},%X{login_user},%X{method},%m%n"-->
      <!--        charset="UTF-8"/>-->
      <PatternLayout
        pattern="%d{yyyy-MM-dd HH:mm:ss} [%t] %-5level %logger{36} - %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <appender-ref ref="console"/>
    </Root>
    <logger name="com.hoan.notificationservice" level="DEBUG"/>
    <logger name="net.ttddyy.dsproxy.listener.logging" level="DEBUG"/>
  </Loggers>
</Configuration>